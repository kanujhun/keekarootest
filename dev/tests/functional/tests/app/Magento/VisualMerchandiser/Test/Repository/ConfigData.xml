<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Magento\Config\Test\Repository\ConfigData">
        <dataset name="visible_attributes_for_category_rules_default">
            <field name="visualmerchandiser/options/smart_attributes" xsi:type="array">
                <item name="scope" xsi:type="string">visualmerchandiser</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="value" xsi:type="array">
                    <item name="Color" xsi:type="string">color</item>
                    <item name="Product Name" xsi:type="string">name</item>
                    <item name="Price" xsi:type="string">price</item>
                    <item name="Quantity" xsi:type="string">quantity_and_stock_status</item>
                    <item name="SKU" xsi:type="string">sku</item>
                </item>
            </field>
        </dataset>
    </repository>
</config>
