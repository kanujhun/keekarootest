<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Magento\Catalog\Test\Repository\CatalogProductAttribute">
        <dataset name="gender_for_target_rule">
            <field name="frontend_label" xsi:type="string">Gender Filter_%isolation%</field>
            <field name="attribute_code" xsi:type="string">gender_filter_%isolation%</field>
            <field name="frontend_input" xsi:type="string">Multiple Select</field>
            <field name="is_required" xsi:type="string">No</field>
            <field name="is_global" xsi:type="string">Global</field>
            <field name="is_used_for_promo_rules" xsi:type="string">Yes</field>
            <field name="options" xsi:type="array">
                <item name="0" xsi:type="array">
                    <item name="is_default" xsi:type="string">Yes</item>
                    <item name="admin" xsi:type="string">girls</item>
                    <item name="view" xsi:type="string">Girls</item>
                </item>
                <item name="1" xsi:type="array">
                    <item name="is_default" xsi:type="string">No</item>
                    <item name="admin" xsi:type="string">boys</item>
                    <item name="view" xsi:type="string">Boys</item>
                </item>
                <item name="2" xsi:type="array">
                    <item name="is_default" xsi:type="string">No</item>
                    <item name="admin" xsi:type="string">men</item>
                    <item name="view" xsi:type="string">Men</item>
                </item>
                <item name="3" xsi:type="array">
                    <item name="is_default" xsi:type="string">No</item>
                    <item name="admin" xsi:type="string">woman</item>
                    <item name="view" xsi:type="string">Woman</item>
                </item>
            </field>
        </dataset>
    </repository>
</config>
